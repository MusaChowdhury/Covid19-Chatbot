<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1" fx:controller="musachowdhury.chatbot.Controller">
    <top>
        <HBox alignment="CENTER_LEFT" spacing="20.0" BorderPane.alignment="CENTER">
            <children>
                <Button id="back" fx:id="back" mnemonicParsing="false" onAction="#back" prefHeight="40.0" prefWidth="40.0" style="-fx-background-color: null;" />
                <Button id="front" fx:id="front" mnemonicParsing="false" onAction="#front" prefHeight="40.0" prefWidth="40.0" style="-fx-background-color: null;" />
                <Button id="reload" fx:id="reload" mnemonicParsing="false" onAction="#reload" prefHeight="40.0" prefWidth="40.0" style="-fx-background-color: null;">
                    <opaqueInsets>
                        <Insets />
                    </opaqueInsets>
                </Button>
            </children>
            <opaqueInsets>
                <Insets />
            </opaqueInsets>
            <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </padding>
        </HBox>
    </top>
</BorderPane>